<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.sinc.shinsteller.index">
	<!-- 메인화면에 보여줄 상품 리스트 조회 -->
	<select id="selectProducts"
			resultType="index">
		SELECT	PRDNO, PRDNAME, PRDTITLE, PRICE
		FROM	SST_PRODUCTS
	</select>
</mapper>  











